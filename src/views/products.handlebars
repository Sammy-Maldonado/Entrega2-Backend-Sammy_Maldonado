<div>
  <h1> Productos Registrados </h1>
  {{#each products}}
  <div>
    <p>{{this.title}}</p>
    <p>{{this.description}}</p>
    <p>{{this.price}}</p>
    <p>{{this.code}}</p>
    <p>{{this.stock}}</p>
    <p>{{this.category}}</p>
    <p>{{this.thumbnails}}</p>
    <p>{{this.status}}</p>
  </div>
  {{/each}}

</div>